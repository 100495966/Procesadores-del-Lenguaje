Grammar

    0 $accept: axioma_programa $end

    1 $@1: ε

    2 axioma_programa: dec_variables $@1 dec_funciones

    3 dec_variables: ε
    4              | INTEGER var_list ';' dec_variables

    5 var_list: var_decl
    6         | var_decl ',' var_list

    7 var_decl: IDENTIF
    8         | IDENTIF '[' NUMBER ']'
    9         | IDENTIF '=' expresion

   10 $@2: ε

   11 dec_funciones: funciones MAIN '(' argumentos ')' '{' $@2 axioma_codigo '}'

   12 funciones: ε
   13          | funcion funciones

   14 $@3: ε

   15 funcion: IDENTIF '(' argumentos ')' '{' $@3 axioma_codigo '}'

   16 argumentos: ε
   17           | INTEGER IDENTIF r_argumentos

   18 r_argumentos: ε
   19             | ',' INTEGER IDENTIF r_argumentos

   20 procedimiento: IDENTIF '(' proc_argumentos ')'

   21 proc_argumentos: ε
   22                | expresion proc_r_argumentos

   23 proc_r_argumentos: ε
   24                  | ',' expresion proc_r_argumentos

   25 axioma_codigo: ε
   26              | sentencia axioma_codigo

   27 sentencia: IDENTIF '=' expresion ';'
   28          | IDENTIF '[' expresion ']' '=' expresion ';'
   29          | INTEGER var_list_local ';'
   30          | PUTS '(' STRING ')' ';'
   31          | PRINTF '(' STRING ',' elems ')' ';'
   32          | WHILE '(' expresion ')' '{' axioma_codigo '}'
   33          | IF '(' expresion ')' '{' axioma_codigo '}'
   34          | IF '(' expresion ')' '{' axioma_codigo '}' ELSE '{' axioma_codigo '}'
   35          | FOR '(' inicializacion ';' expresion ';' inc_dec ')' '{' axioma_codigo '}'
   36          | procedimiento ';'
   37          | RETURN expresion ';'

   38 var_list_local: var_decl_local
   39               | var_decl_local ',' var_list_local

   40 var_decl_local: IDENTIF
   41               | IDENTIF '[' NUMBER ']'
   42               | IDENTIF '=' expresion

   43 inicializacion: IDENTIF '=' NUMBER

   44 inc_dec: IDENTIF '=' IDENTIF inc_dec_operadores NUMBER

   45 inc_dec_operadores: '+'
   46                   | '-'
   47                   | '*'
   48                   | '/'
   49                   | '%'

   50 elems: elem r_elems

   51 r_elems: ε
   52        | ',' elem r_elems

   53 elem: expresion
   54     | STRING

   55 expresion: termino
   56          | expresion '+' expresion
   57          | expresion '-' expresion
   58          | expresion '*' expresion
   59          | expresion '/' expresion
   60          | expresion '%' expresion
   61          | expresion AND expresion
   62          | expresion OR expresion
   63          | expresion NEQ expresion
   64          | expresion EQ expresion
   65          | expresion '<' expresion
   66          | expresion LE expresion
   67          | expresion '>' expresion
   68          | expresion GE expresion
   69          | '!' expresion

   70 termino: operando
   71        | '+' operando
   72        | '-' operando

   73 operando: IDENTIF
   74         | NUMBER
   75         | IDENTIF '[' expresion ']'
   76         | procedimiento
   77         | '(' expresion ')'


